{
  "title": "when tab alone to fn",
  "rules": [
	{
	  "description": "when tab alone do fn",
	  "manipulators": [
        {
    "type": "basic",
    "from": {
        "key_code": "tab",
        "modifiers": {
            "optional": [
                "any"
            ]
        }
    },
    "to": [
        {
            "key_code": "fn",
            "lazy": true
        }
    ],
    "to_if_alone": [
        {
            "key_code": "tab"
        }
    ]
}
	  ]
	}
  ]
}
